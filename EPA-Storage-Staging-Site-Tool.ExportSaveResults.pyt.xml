	<tool xmlns="" toolboxalias="" displayname="Export/Save Results" name="ExportSaveResults">
		<arcToolboxHelpPath>c:\program files\arcgis\pro\Resources\Help\gp</arcToolboxHelpPath>
		<parameters>
			<param displayname="Scenario ID" name="ScenarioID" datatype="GPString" direction="Input" type="Required">
				<dialogReference><DIV STYLE="text-align:Left;"><DIV><P><SPAN>Select the scenario for which you would like to export a report.</SPAN></P></DIV></DIV></dialogReference>
			</param>
			<param displayname="Map Layout Graphic" name="MapLayoutGraphic" datatype="GPString" direction="Input" type="Required">
